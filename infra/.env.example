# ============================================================
# SER-004: 시크릿 외부화 — 환경변수 템플릿
# .env 파일로 복사 후 실제 값을 설정하세요.
# 주의: .env 파일은 절대 git에 커밋하지 마세요.
# ============================================================

# ── PostgreSQL (Airflow/MLflow 공용) ──
POSTGRES_USER=asan
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=asan_db

# ── OMOP CDM DB ──
OMOP_DB_HOST=localhost
OMOP_DB_PORT=5436
OMOP_DB_USER=omopuser
OMOP_DB_PASSWORD=CHANGE_ME_OMOP_PASSWORD

# ── Redis ──
REDIS_PASSWORD=CHANGE_ME_REDIS_PASSWORD

# ── Airflow ──
AIRFLOW_FERNET_KEY=GENERATE_WITH_python_-c_"from_cryptography.fernet_import_Fernet;_print(Fernet.generate_key().decode())"
AIRFLOW_SECRET_KEY=CHANGE_ME_AIRFLOW_SECRET
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=CHANGE_ME_AIRFLOW_PASSWORD

# ── Superset ──
SUPERSET_SECRET_KEY=CHANGE_ME_SUPERSET_SECRET
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=CHANGE_ME_SUPERSET_PASSWORD

# ── JWT (인증) ──
JWT_SECRET=CHANGE_ME_JWT_SECRET_KEY_MIN_32_CHARS
AUTH_REQUIRED=true

# ── LLM ──
LLM_API_URL=http://localhost:28888/v1
LLM_MODEL=Qwen3-32B-AWQ
OPENAI_API_KEY=
EMBEDDING_API_URL=http://localhost:8082

# ── Milvus Vector DB ──
MILVUS_HOST=localhost
MILVUS_PORT=19530

# ── MinIO S3 Object Storage ──
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=CHANGE_ME_MINIO_PASSWORD
MINIO_ENDPOINT=localhost:19000
